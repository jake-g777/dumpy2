{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "main": "electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"powershell ./start-server.ps1\"",
    "dev:frontend": "echo '=== Frontend Server ===\nURL: http://localhost:5173\n==================' && vite",
    "dev:backend": "powershell ./start-server.ps1",
    "dev:server": "cd server && npx nodemon src/server.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on tcp:5173 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:preview": "npm run build && electron ."
  },
  "dependencies": {
    "@types/animejs": "^3.1.13",
    "@types/cors": "^2.8.18",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.1",
    "@types/papaparse": "^5.3.16",
    "@types/pg": "^8.15.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/xlsx": "^0.0.35",
    "animejs": "^4.0.2",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "firebase": "^11.7.1",
    "graphql-ws": "^6.0.5",
    "knex": "^3.1.0",
    "lucide-react": "^0.344.0",
    "mysql2": "^3.14.1",
    "nodejs": "^0.0.0",
    "nodemon": "^3.1.10",
    "papaparse": "^5.5.2",
    "pg": "^8.16.0",
    "pg-copy-streams": "^6.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.0",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^3.3.0",
    "ts-node": "^10.9.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.1.2",
    "electron": "^36.2.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.19",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.electron.your-app",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ]
  }
}
